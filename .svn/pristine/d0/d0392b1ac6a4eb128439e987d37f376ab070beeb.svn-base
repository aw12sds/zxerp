/**载入初始化页面开始**/
.loading-indicator {
	font-size: 11px;
	background-image: url(../deng/images/load/loading.gif);
	background-repeat: no-repeat;
	background-position: top left;
	padding-left: 20px;
	height: 18px;
	text-align: left;
}
#loading-mask {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	z-index: 20000;
	background-color: #FFFFFF !important;
}
#loading {
	border: 1px solid #f5f5f5;
	position: absolute;
	left: 40%;
	top: 40%;
	padding: 2px;
	z-index: 20001;
	width: 250px;
	height: auto;
}
#loading img {
	margin-bottom: 5px;
}
#loading .loading-indicator {
	background: white;
	border: 0px solid #f5f5f5;
	color: #555;
	font: bold 13px helvetica, arial, tahoma, sans-serif;
	padding: 10px;
	margin: 0;
	text-align: center;
	height: auto;
}
/**载入初始化页面结束**/

